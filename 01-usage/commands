watchpoint set variable count
proces continue
proces continue
expression -- tab[0] = tab[1]
proces continue
expression int $tmp = tab[1]
proces continue
expression -- tab[2] = tab[1]; tab[1] = $tmp
proces continue
expression -- count = 0
watchpoint delete 1
breakpoint set -p "return tmp"|"return biggest"
proces continue
expression -- for(int i = 0; i < max; i++) tmp += min[i]; tmp /= max;
proces continue
expression -- for(int i = 0; i < max; i++) if(biggest < min[i]) biggest = min[i]
proces continue
